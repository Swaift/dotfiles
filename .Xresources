!urxvt*background: Black
!urxvt*foreground: #ebdbb2
urxvt*scrollBar: false
urxvt*scrollTtyOutput: false
urxvt*scrollWithBuffer: true
urxvt*scrollWithKeypress: true
urxvt*saveLines: 100000


!urxvt*transparent: true
!urxvt*shading: 30
urxvt*depth: 32
urxvt*background: rgba:0000/0000/0200/C800
urxvt*font: xft:operatormono:size=9.8
urxvt*letterSpace: -1
urxvt*buffered: false
urxvt*perl-ext-common: default,clipboard
urxvt*keysym.Shift-Control-V: perl:clipboard:paste
urxvt*iso14755: false
urxvt*internalBorder: 40

!
! hard contrast: *background: #1d2021
*background: #282828
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ffffff

*cursorColor: #ebdbb2
